{
  "rules": {},
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
  ],
  "plugins": [
    "react"
  ],
  "globals": {
    "async": false,
    "mApi": false,
    "moment": false,
    "formatDate": false,
    "formatTime": false,
    "renderDustTemplate": false,
    "getLocaleText": false,
    "MUIKKU_LOGGED_USER_ID": false,
    "MUIKKU_LOGGEDIN": false
  }
}
