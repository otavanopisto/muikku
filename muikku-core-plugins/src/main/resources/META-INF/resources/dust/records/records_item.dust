
<div class="mf-item tr-item ft-01" data-workspace-entity-grade="{grade}"  data-workspace-entity-name="{name|s}"  data-workspace-entity-description="{description|s}" data-workspace-entity-id="{id}" data-workspace-entity-url-name="{urlName}" data-workspace-verbal-assessment="{verbalAssessment|s}" data-workspace-grade="{grade}" data-workspace-grading-scale="{gradingScale}" data-workspace-evaluated="{evaluated}">
    <div class="tr-item-details">

      <div class="tr-item-details-container">       
        <div class="tr-item-additionalInfo">

        </div>
        <div class="tr-item-description">
          <div class="tr-item-header-name">
           <span class="tr-item-long">{name}</span>
          </div>
          
          <div class="tr-item-header-details">
            {#grade}
              <div class="tr-item-header-detail date"><span>{#localize key="plugin.records.workspace.evaluated" arg0=evaluated /}</div>
              <div class="tr-item-header-detail grade" title="{gradingScale}"><span>{grade}</span></div>
            {/grade}
          </div>
                  
        </div>       

        <div class="clear"></div>
      </div> 
    </div>
  </div>
 
